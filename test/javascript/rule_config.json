{
  "checkerId": "taintChecker",
  "Rules": [
    {
      "type": "taint_flow_test",
      "desc": "Test for taint flow.",
      "advice": "Avoid flowing the source to the taint sink",
      "severity": 5
    },
    {
      "type": "taint_flow_js",
      "desc": "taint flow for javascript",
      "advice": "Avoid flowing the source to the taint sink",
      "severity": 1
    }
  ],
  "TaintSource": [
    {
      "path": "__taint_src",
      "kind": "TEST",
      "introPoint": 1
    },
    {
      "path": "__taint_src",
      "kind": "JS_INPUT",
      "introPoint": 1
    }
  ],
  "FuncCallTaintSource": [
    {
      "fsig": "__taint_sink",
      "args": [
        "*"
      ],
      "kind": "TEST"
    }
  ],
  "FunctionCallSanitizer": [
    {
      "fsig": "validateTaint",
      "args": [
        "*"
      ],
      "from": "P0",
      "to": ["R","P0"],
      "kind": "TEST",
      "isContinue": true
    }
  ],
  "FuncCallTaintSink": [
    {
      "fsig": "__taint_sink",
      "args": [
        0
      ],
      "kind": "TEST"
    },
    {
      "fsig": "__taint_sink",
      "args": [
        0
      ],
      "kind": "JS_INPUT"
    }
  ],
  "AppName": "jsxastbenchmark"
}
